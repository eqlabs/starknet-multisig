    "@types/mocha": "^9.0.0",
    "@types/node": "^16.11.10",
    "chai": "^4.3.4",
    "ethers": "^5.4.6",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.2"
  },
  "dependencies": {
    "@toruslabs/starkware-crypto": "^1.0.0",
    "hardhat": "2.9.0",
    "starknet": "^3.5.1"
  }
