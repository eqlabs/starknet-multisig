  "private": false,
  "scripts": {
    "compile": "npx hardhat starknet-compile contracts && npx hardhat run scripts/copyContractsToFrontend.ts",
    "deploy:multisig": "npx hardhat starknet-deploy --starknet-network devnet ./starknet-artifacts/Multisig.cairo --inputs \"1 0x5aca08c8e5e5bccfe3a83df289887c619eebbececa111f4ef79d9419fd16bcf 1\"",
    "deploy:target": "npx hardhat starknet-deploy --starknet-network devnet ./starknet-artifacts/mock/Target.cairo",
    "local": "starknet-devnet --lite-mode"
  },
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "^2.0.5",
    "@shardlabs/starknet-hardhat-plugin": "0.5.2",
